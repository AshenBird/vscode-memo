{
	"name": "notebook.kit",
	"displayName": "McSwift Notebook",
	"description": "",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.62.3"
	},
	"publisher": "McSwift",
	"categories": [
		"Other"
	],
	"main": "./out/host/index.js",
	"activationEvents": [
		"onView:mcswift-memo",
		"onView:mcswift-todo",
		"onStartupFinished"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "mcswift-notebook",
					"title": "notebook",
					"icon": "public/notebook.svg"
				}
			]
		},
		"views": {
			"mcswift-notebook": [
				{
					"id": "mcswift-memo",
					"name": "便笺",
					"type": "webview"
				}
			],
			"mcswift-todo": [
				{
					"id": "mcswift-todo",
					"name": "待办"
				}
			]
		}
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/AshenBird/vscode-memo"
	},
	"scripts": {
		"pub": "vsce publish --yarn",
		"vscode:prepublish": "npm run build",
		"build": "node scripts/build.js",
		"watch": "node scripts/watch.js",
		"watch:host": "cd src/host && tsc -watch -p ./",
		"build:host": "cd src/host && tsc -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vitejs/plugin-vue": "^1.10.0",
		"@vitejs/plugin-vue-jsx": "^1.3.0",
		"@vscode/test-electron": "^1.6.2",
		"cross-env": "^7.0.3",
		"esbuild": "^0.13.13",
		"eslint": "^8.1.0",
		"execa": "^5.1.1",
		"fs-extra": "^10.0.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"sass": "^1.43.4",
		"typescript": "^4.4.4",
		"vite": "^2.6.14"
	},
	"dependencies": {
		"vue": "^3.2.22"
	}
}