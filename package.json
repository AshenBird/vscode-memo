{
	"name": "notebook",
	"displayName": "McSwift Notebook",
	"description": "",
	"version": "0.1.0",
	"engines": {
		"vscode": "^1.63.1"
	},
	"publisher": "McSwift",
	"categories": [
		"Other",
		"Extension Packs"
	],
	"main": "./out/host/index.js",
	"activationEvents": [
		"onStartupFinished"
	],
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "mcswift-notebook",
					"title": "Notebook",
					"icon": "public/notebook.svg"
				}
			]
		},
		"views": {
			"mcswift-notebook": [
				{
					"id": "mcswift-note-explorer",
					"name": "目录",
					"type": "webview"
				}
			]
		}
	},
	"extensionPack": [
		"tomoki1207.pdf",
		"hediet.vscode-drawio",
		"gera2ld.markmap-vscode",
		"csholmq.excel-to-markdown-table",
		"vscode-icons-team.vscode-icons",
		"mcswift.markdown"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/AshenBird/vscode-note"
	},
	"scripts": {
		"pub": "vsce publish --yarn",
		"vscode:prepublish": "npm run build",
		"build": "node scripts/build.js",
		"watch": "node scripts/watch.js",
		"build:client": "cd src/client/explorer && cross-env MODE=build vite-ssg build",
		"watch:host": "cd src/host && tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vicons/fa": "^0.11.0",
		"@vicons/tabler": "^0.11.0",
		"@vitejs/plugin-vue": "^1.10.0",
		"@vitejs/plugin-vue-jsx": "^1.3.0",
		"@vscode/test-electron": "^1.6.2",
		"@vueuse/head": "^0.7.4",
		"cross-env": "^7.0.3",
		"esbuild": "^0.13.13",
		"eslint": "^8.1.0",
		"execa": "^5.1.1",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"sass": "^1.43.4",
		"typescript": "^4.4.4",
		"vite": "^2.6.14",
		"vite-ssg": "^0.17.0",
		"vsce": "^2.5.0",
		"vue-router": "^4.0.12",
		"zx": "^4.2.0"
	},
	"dependencies": {
		"@vicons/ionicons5": "^0.11.0",
		"fs-extra": "^10.0.0",
		"naive-ui": "^2.21.1",
		"vfonts": "^0.1.0",
		"vue": "^3.2.22"
	}
}